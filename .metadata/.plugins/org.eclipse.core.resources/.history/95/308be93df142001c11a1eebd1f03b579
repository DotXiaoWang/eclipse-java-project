package chapter2_1;

public class Book {
	String isbn;
	String name;
	String price;
	public booblean equals(Object obj){
		if(this == obj){
			return true;
		}
		if(obj==null){
			return false;
			
		}
		if(getClass()!=obj.getClass()){
			
		}
		Book other = (Book)obj;
		if(isbn==null){
			if(other.isbn!=null){
				return false;
			}
			
		}
		
		
	}
	

}
